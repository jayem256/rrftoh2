{
  "server": {
    "hostName": "Test",
    "bindAddr": "0.0.0.0",
    "port": 443,
    "cert": "./certs/server.crt",
    "key": "./certs/server.key",
    "windowSize": 104857600
  },
  "client": {
    "certPool": ["./certs/client.crt"]
  },
  "file": {
    "docRoot": "./",
    "bufferedRead": 524288
  },
  "compression": {
    "enableGzip": true,
    "compressionTreshold": 10485760,
    "gzipBuffer": 4194304,
    "omitExtensions": [
      "zip", "jpg", "mkv", "png", "tar", "gz", 
      "rar", "mp4", "avi", "gif", "mp3", "webp"
    ]
  }
}